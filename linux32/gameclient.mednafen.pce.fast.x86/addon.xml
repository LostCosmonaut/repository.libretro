<?xml version="1.0" encoding="UTF-8"?>
<addon id="gameclient.mednafen.pce.fast.x86"
        name="Mednafen PC Engine (Fast) (32-bit)"
        version="1.0.2"
        provider-name="Libretro">
    <requires>
        <import addon="xbmc.libretro" version="1.0.0"/>
    </requires>
    <extension point="xbmc.gameclient"
            library_android=""
            library_linux="libretro-mednafen-pce-fast.so"
            library_osx=""
            library_windx=""
            library_wingl="">
        <platforms>TurboGrafx-16|TurboGrafx CD</platforms>
        <extensions>pce|cue|zip</extensions>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Mednafen PC Engine / TurboGrafx 16 / SuperGrafx</summary>
        <description>Mednafen is a portable multi-system emulator. The "pce_fast" Mednafen emulation module is an experimental alternative to the pce emulation module, modified for speed at the expense of (usually) unneeded accuracy.

Supported files: .pce, .cue, .zip

Source: https://github.com/libretro/mednafen-libretro</description>
        <noicon>true</noicon>
        <nofanart>true</nofanart>
        <platform>linux</platform>
    </extension>
</addon>
