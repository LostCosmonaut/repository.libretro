<?xml version="1.0" encoding="UTF-8"?>
<addon id="gameclient.mednafen.pce.fast"
        name="Mednafen PC Engine (Fast)"
        version="1.0.1"
        provider-name="Libretro">
    <requires>
        <import addon="xbmc.libretro" version="1.0.0"/>
    </requires>
    <extension point="xbmc.gameclient"
            library_android=""
            library_linux="libretro-mednafen-pce-fast.so"
            library_osx=""
            library_windx=""
            library_wingl="">
        <platforms>TurboGrafx-16|TurboGrafx CD</platforms>
        <extensions>pce|cue|zip</extensions>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Mednafen PC Engine / TurboGrafx 16 / SuperGrafx</summary>
        <description>Mednafen is a portable multi-system emulator. This is port of the Mednafen PCE core to the libretro API. The "pce_fast" emulation module is an experimental alternative to the pce emulation module. It is a fork of 0.8.x modified for speed at the expense of (usually) unneeded accuracy.[CR][CR]Source: https://github.com/libretro/mednafen-libretro</description>
        <noicon>true</noicon>
        <nofanart>true</nofanart>
        <platform>linux</platform>
    </extension>
</addon>
