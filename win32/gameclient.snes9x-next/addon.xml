<?xml version="1.0" encoding="UTF-8"?>
<addon id="gameclient.snes9x-next"
        name="Snes9x-Next"
        version="1.0.2"
        provider-name="Libretro">
    <requires>
        <import addon="xbmc.libretro" version="1.0.0"/>
    </requires>
    <extension point="xbmc.gameclient"
            library_android=""
            library_linux=""
            library_osx=""
            library_windx="libretro-git-snes9x-next.dll"
            library_wingl="libretro-git-snes9x-next.dll">
        <platforms>SNES</platforms>
        <extensions>smc|fig|sfc|gd3|gd7|dx2|bsx|swc|zip</extensions>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Snes9x-Next</summary>
        <description>Snes9x-Next is based on Snes9x, but has been heavily modified/speed-hacked to achieve better speed.

Supported files: .smc, .fig, .sfc, .gd3, .gd7, .dx2, .bsx, .swc, .zip

Source: https://github.com/libretro/snes9x-next</description>
        <nofanart>true</nofanart>
        <platform>windx wingl</platform>
    </extension>
</addon>
